<!doctype html>
<html lang="en" dir="ltr">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta http-equiv="Content-Language" content="en" />
    <meta name="msapplication-TileColor" content="#2d89ef">
    <meta name="theme-color" content="#4188c9">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent" />
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="HandheldFriendly" content="True">
    <meta name="MobileOptimized" content="320">
    <link rel="icon" href="../public/BLOG.PNG" type="image/x-icon" />
    <link rel="shortcut icon" type="image/x-icon" href="../public/BLOG.PNG" />
    <!-- Generated: 2019-04-04 16:55:45 +0200 -->
    <title>BLog</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,300i,400,400i,500,500i,600,600i,700,700i&amp;subset=latin-ext">
    <script src="../public/assets/js/require.min.js"></script>
    <script>
        requirejs.config({
            baseUrl: '.'
        });

    </script>
    <!-- Dashboard Core -->
    <link href="../public/assets/css/dashboard.css" rel="stylesheet" />
    <script src="../public/assets/js/dashboard.js"></script>
    <!-- c3.js Charts Plugin -->
    <link href="../public/assets/plugins/charts-c3/plugin.css" rel="stylesheet" />
    <script src="../public/assets/plugins/charts-c3/plugin.js"></script>
    <!-- Google Maps Plugin -->
    <link href="../public/assets/plugins/maps-google/plugin.css" rel="stylesheet" />
    <script src="../public/assets/plugins/maps-google/plugin.js"></script>
    <!-- Input Mask Plugin -->
    <script src="../public/assets/plugins/input-mask/plugin.js"></script>
    <!-- Datatables Plugin -->
    <script src="../public/assets/plugins/datatables/plugin.js"></script>
</head>

<body class="">
    <div class="page">
        <div class="flex-fill">
            <div class="header py-4">
                <div class="container">
                    <div class="d-flex">
                        <a class="header-brand" href="./Users">
                            <img src="../public/BLOG.PNG" class="header-brand-img" alt="tabler logo"> BLog
                        </a>
                    </div>
                </div>
            </div>
            <div class="header collapse d-lg-flex p-0" id="headerMenuCollapse">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-lg order-lg-first">
                            <ul class="nav nav-tabs border-0 flex-column flex-lg-row">
                                <li class="nav-item">
                                    <a href="./Users" class="nav-link active"><i class="fe fe-user"></i> Users</a>
                                </li>
                                <li class="nav-item">
                                    <a href="./Logs" class="nav-link"><i class="fe fe-file-text"></i> Logs</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="my-3 my-md-5">
                <div class="container">
                    <div class="row row-cards">
                        <div class="col-12">
                            <div class="card">
                                <div class="card-header">
                                    <h3 class="card-title">Users</h3>
                                </div>

                                <div class="table-responsive">
                                    <table class="table card-table table-vcenter text-nowrap datatable">
                                        <thead>
                                            <tr>
                                                <th class="w-1">No.</th>
                                                <th>Name</th>
                                                <th>IP</th>
                                                <th>Log Safety</th>
                                                <th class="w-2">Logs</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each result}}
                                            <tr>
                                                <td><span class="text-muted">{{this.User_No}}</span></td>
                                                <td>{{this.User_Name}}</td>
                                                <td>
                                                   {{this.User_IP}}
                                                </td>
                                                <td>
                                                    <div class="clearfix">
                                                        <div class="float-left">
                                                            <strong>100%</strong>
                                                        </div>
                                                        <div class="float-right">
                                                            <small class="text-muted">16 Dec, 2019 - 19 Dec, 2019</small>
                                                        </div>
                                                    </div>
                                                    <div class="progress progress-xs">
                                                        <div class="progress-bar bg-green" role="progressbar" style="width: 100%" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
                                                    </div>
                                                </td>
                                                <td align=right>
                                                    <a class="icon" href="./UserLogs/{{this.User_No}}">
                                                        <i class="fe fe-edit"></i> View Logs
                                                    </a>
                                                </td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                    </table>
                                    <script>
                                        require(['datatables', 'jquery'], function(datatable, $) {
                                            $('.datatable').DataTable();
                                        });

                                    </script>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
        <footer class="footer">
            <div class="container">
                <div class="row align-items-center flex-row-reverse">
                    <div class="col-auto ml-lg-auto">
                        <div class="row align-items-center">

                        </div>
                    </div>
                    <div class="col-12 col-lg-auto mt-3 mt-lg-0 text-center">
                        Copyright Â© 2019 <a href=".">Blockchain Log</a> All rights reserved.
                    </div>
                </div>
            </div>
        </footer>
    </div>
</body>

</html>
