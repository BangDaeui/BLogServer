<body class="">
    <div class="page">
        <div class="flex-fill">
            <div class="header py-4">
                <div class="container">
                    <div class="d-flex">
                        <a class="header-brand" href="./Users">
                            <img src="../public/BLOG.PNG" class="header-brand-img" alt="tabler logo"> BLog
                        </a>
                    </div>
                </div>
            </div>
            <div class="header collapse d-lg-flex p-0" id="headerMenuCollapse">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-lg order-lg-first">
                            <ul class="nav nav-tabs border-0 flex-column flex-lg-row">
                                <li class="nav-item">
                                    <a href="/Users" class="nav-link active"><i class="fe fe-user"></i> Users</a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Logs" class="nav-link"><i class="fe fe-calendar"></i> Logs</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="my-3 my-md-5">
                <div class="container">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title"><b>{{#each result1}}{{this.User_Name}}{{/each}}</b></h3>
                            <div class="card-options">
                                <button type="button" class="btn btn-indigo" onclick="javascript:window.print();"><i class="fe fe-printer"></i> Print Logs</button>
                            </div>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive push">
                                <table class="table table-bordered table-hover">
                                    <tr>
                                        <th>User No</th>
                                        <th>User Name</th>
                                        <th>User IP</th>
                                        <th>Last Log Update</th>
                                    </tr>
                                    {{#each result1}}
                                    <tr>
                                        <td>{{this.User_No}}</td>
                                        <td>{{this.User_Name}}</td>
                                        <td>{{this.User_IP}}</td>
                                        <td>{{this.User_Log}}</td>
                                    </tr>
                                    {{/each}}
                                </table>
                            </div>
                            <div class="table-responsive push">
                                <table class="table table-bordered table-hover">
                                    <tr>
                                        <th class="text-left" style="width: 1%">No</th>
                                        <th class="text-left">Log File Name</th>
                                        <th class="text-left" style="width: 10%">Created</th>
                                        <th class="text-left" style="width: 1%">Hash Value</th>
                                        <th class="text-left" style="width: 1%">Blockchain Hash Value</th>
                                        <th class="text-left" style="width: 10%">Check</th>
                                    </tr>
                                    {{#each result2}}
                                    <tr>
                                        <td class="text-center">{{this.App_No}}</td>
                                        <td class="text-left">
                                            <p class="font-w600 mb-1">{{this.App_Name}}</p>
                                        </td>
                                        <td class="text-left">
                                            {{this.App_Time}}
                                        </td>
                                        <td class="text-left">{{this.App_Hash}}</td>
                                        <td class="text-left">{{this.App_Hash}}</td>
                                        <td class="text-left"><span class="status-icon bg-success"></span>Good</td>
                                    </tr>
                                    {{/each}}
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <footer class="footer">
            <div class="container">
                <div class="row align-items-center flex-row-reverse">
                    <div class="col-auto ml-lg-auto">
                        <div class="row align-items-center">

                        </div>
                    </div>
                    <div class="col-12 col-lg-auto mt-3 mt-lg-0 text-center">
                        Copyright Â© 2019 <a href=".">Blockchain Log</a> All rights reserved.
                    </div>
                </div>
            </div>
        </footer>
    </div>
</body></html>
